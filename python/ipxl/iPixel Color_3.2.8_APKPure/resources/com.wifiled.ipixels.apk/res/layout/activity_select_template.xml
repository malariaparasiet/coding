<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/main"
    android:background="#37393c"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/il_text_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent">
            <com.wifiled.ipixels.view.customview.CustomImageView
                android:id="@+id/iv_back"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="18sp"
                android:textColor="@color/white"
                android:ellipsize="marquee"
                android:gravity="center"
                android:id="@+id/tv_title"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="2"
                android:marqueeRepeatLimit="marquee_forever"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.wifiled.ipixels.view.customview.CustomImageView
                android:id="@+id/iv_right_subtitle"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.wifiled.ipixels.view.customview.CustomImageView
                android:id="@+id/iv_middle"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/dp_10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/iv_right"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.wifiled.ipixels.view.customview.CustomImageView
                android:id="@+id/iv_right"
                android:visibility="visible"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/dp_10"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <View
                android:background="#252628"
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_back"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_1"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv1"
                android:background="#272829"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="3.3dp"
                android:text="Aa"
                app:layout_constraintBottom_toTopOf="@+id/tem1_tv2"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv2"
                android:background="#272829"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="Aa"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tem1_tv1"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_2"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <ImageView
                android:id="@+id/tem1_iv1"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                android:layout_marginEnd="3.3dp"
                app:layout_constraintEnd_toStartOf="@+id/tem1_tv3"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv3"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Aa"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_iv1"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_3"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv4"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Aa"
                android:layout_marginEnd="3.3dp"
                app:layout_constraintEnd_toStartOf="@+id/tem1_iv2"
                app:layout_constraintStart_toStartOf="parent"/>
            <ImageView
                android:id="@+id/tem1_iv2"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_tv4"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_4"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <ImageView
                android:id="@+id/tem1_iv3"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                app:layout_constraintEnd_toStartOf="@+id/tem1_tv5"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv5"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="Aa"
                android:layout_marginStart="3.3dp"
                app:layout_constraintBottom_toTopOf="@+id/tem1_tv6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_iv3"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv6"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="3.3dp"
                android:text="Aa"
                android:layout_marginStart="3.3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_iv3"
                app:layout_constraintTop_toBottomOf="@+id/tem1_tv5"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_5"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv7"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="Aa"
                android:layout_marginEnd="3.3dp"
                app:layout_constraintBottom_toTopOf="@+id/tem1_tv8"
                app:layout_constraintEnd_toStartOf="@+id/tem1_iv4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textColor="#ababab"
                android:gravity="center"
                android:id="@+id/tem1_tv8"
                android:background="#272829"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="3.3dp"
                android:text="Aa"
                android:layout_marginEnd="3.3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tem1_iv4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tem1_tv7"/>
            <ImageView
                android:id="@+id/tem1_iv4"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_tv7"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tem_6"
            android:background="#4d5053"
            android:padding="3.3dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="78dp"
            android:layout_marginTop="13dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp">
            <ImageView
                android:id="@+id/tem1_iv5"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                app:layout_constraintEnd_toStartOf="@+id/tem1_iv6"
                app:layout_constraintStart_toStartOf="parent"/>
            <ImageView
                android:id="@+id/tem1_iv6"
                android:background="#313336"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:src="@drawable/template_img"
                android:layout_marginStart="3.3dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/tem1_iv5"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
