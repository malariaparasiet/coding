<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/template"
        android:background="#4d5053"
        android:padding="3.3dp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,2:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:id="@+id/tem6_iv1"
            android:background="#313336"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/template_img"
            app:layout_constraintEnd_toStartOf="@+id/tem6_iv2"
            app:layout_constraintStart_toStartOf="parent"/>
        <View
            android:background="#4d5053"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tem6_iv2"
            app:layout_constraintStart_toEndOf="@+id/tem6_iv1"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/tem6_iv2"
            android:background="#313336"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/template_img"
            android:layout_marginStart="3.3dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tem6_iv1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
