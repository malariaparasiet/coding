<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#4d5053"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/template"
        android:padding="2dp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H,4:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textColor="#ababab"
            android:gravity="center"
            android:id="@+id/tem3_tv1"
            android:background="#272829"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="Aa"
            android:layout_marginEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tem3_iv1"
            app:layout_constraintStart_toStartOf="parent"/>
        <View
            android:id="@+id/tem3_border_view1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/tem3_tv1"
            app:layout_constraintEnd_toEndOf="@+id/tem3_tv1"
            app:layout_constraintStart_toStartOf="@+id/tem3_tv1"
            app:layout_constraintTop_toTopOf="@+id/tem3_tv1"/>
        <RelativeLayout
            android:gravity="center"
            android:id="@+id/tem3_tv1_text"
            android:background="#000"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/tem3_tv1"
            app:layout_constraintEnd_toEndOf="@+id/tem3_tv1"
            app:layout_constraintStart_toStartOf="@+id/tem3_tv1"
            app:layout_constraintTop_toTopOf="@+id/tem3_tv1">
            <com.wifiled.ipixels.ui.channel.text.ObjectarxItem
                android:id="@+id/tem3_tv1_text_rl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <ImageView
            android:id="@+id/tem3_iv1"
            android:background="#313336"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/template_img"
            app:layout_constraintDimensionRatio="H,1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tem3_tv1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
